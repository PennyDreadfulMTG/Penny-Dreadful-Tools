{{#show_archetype_tree}}
    <section>
        <h2>Archetype Tree</h2>
        {{> archetypetree}}
    </section>
{{/show_archetype_tree}}
{{#has_most_common_cards}}
    <section>
        <h2>Most Common Cards</h2>
        <ul>
            {{#most_common_cards}}<li>{{> singlecard}}</li>{{/most_common_cards}}
        </ul>
    </section>
{{/has_most_common_cards}}
{{#show_record}}
    <section>
        <h2>Matchups</h2>
        {{#matchups_container}}
            {{> archetypetree}}
        {{/matchups_container}}
    </section>
{{/show_record}}
<section>
    <h2>All Decks</h2>
    {{> livedecktable}}
</section>
