<tr data-href="{{url}}" class="clickable">
    <td class="marginalia">{{{stars_safe}}}</td>
    <td class="contains-mana-bar">{{{colors_safe}}}</td>
    <td><a {{#decklist}}title="{{decklist}}"{{/decklist}}href="{{url}}">{{{name}}}</a></td>
    {{^hide_person}}
        <td><a href="{{person_url}}" class="person">{{person}}</a></td>
    {{/hide_person}}
    {{#show_archetype}}
        <td><a href="{{archetype_url}}">{{archetype_name}}</a></td>
    {{/show_archetype}}
    {{^hide_source}}
        <td {{#source_sort}}data-text="{{source_sort}}"{{/source_sort}}>
            {{#competition_url}}<a href="{{competition_url}}">{{/competition_url}}
            {{{source_name}}}
            {{#competition_url}}</a>{{/competition_url}}
        </td>
    {{/hide_source}}
    <td class="n">
        {{#competition_url}}<a href="{{competition_url}}">{{/competition_url}}
        {{> record}}
        {{#competition_url}}</a>{{/competition_url}}
    </td>
    {{#show_omw}}
        <td class="n">{{omw}}</td>
    {{/show_omw}}
    {{^hide_top8}}
        <td class="c">
            {{#top8_safe}}<a href="{{competition_url}}">{{/top8_safe}}
            {{{top8_safe}}}
            {{#top8_safe}}</a>{{/top8_safe}}
        </td>
    {{/hide_top8}}
    <td data-text="{{date_sort}}">
        {{display_date}}
    </td>
    {{#show_season_icon}}
        <td>
            {{{season_icon}}}
        </td>
    {{/show_season_icon}}
</tr>
